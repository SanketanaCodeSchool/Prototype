{
  "name": "test-simple-react-admin",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.8.3",
    "@mui/material": "^5.8.2",
    "firebase": "^9.6.4",
    "firebase-admin": "^10.2.0",
    "firebase-functions": "^3.21.2",
    "ra-input-rich-text": "^3.3.1",
    "react": "^17.x",
    "react-admin": "^3.19.10",
    "react-admin-firebase": "3",
    "react-dom": "^17.x",
    "react-final-form": "^6.3.0",
    "react-firebaseui": "^6.0.0",
    "react-router-dom": "^5.x",
    "react-scripts": "^5.0.0",
    "source-map-loader": "^3"
  },
  "homepage": ".",
  "devDependencies": {
    "concurrently": "4.1.1",
    "cpx": "1.5.0",
    "env-cmd": "^10.1.0",
    "gh-pages": "3.2.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:dev": "env-cmd -f .env.dev react-scripts start",
    "start:prod": "env-cmd -f .env.prod react-scripts start",
    "build": "react-scripts build",
    "build:dev": "env-cmd -f .env.dev npm run build",
    "build:prod": "env-cmd -f .env.prod npm run build && firebase deploy -P prod",
    "build-ci": "react-scripts build",
    "cp-dist": "rm -rf ./node_modules/react-admin-firebase/dist && cp -r ../dist ./node_modules/react-admin-firebase/dist",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "deploy": "gh-pages -d build",
    "env": "cp env.${GCLOUD_PROJECT:-local}.js public/env.js"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
